<?xml version="1.0" encoding="UTF-8"?>
<Patch>
  <Operation Class="XmlExtensions.FindMod">
    <mods>
      <li>vanillaexpanded.vfefarming</li>
    </mods>
    <packageId>True</packageId>
    <caseTrue>
      <Operation Class="XmlExtensions.OptionalPatch">
        <modId>Rhynia.Mod.Misc</modId>
        <key>VE_VFEF_EcosystemPowerful</key>
        <defaultValue>False</defaultValue>
        <caseTrue>
          <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vfefarming">
            <xpath>/Defs/ThingDef[defName = "VFE_Ecosystem"]/fertility</xpath>
            <value>
              <fertility>8</fertility>
            </value>
          </Operation>
          <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vfefarming">
            <xpath>/Defs/ThingDef[defName = "VFE_Ecosystem_Tilable"]/fertility</xpath>
            <value>
              <fertility>8</fertility>
            </value>
          </Operation>
        </caseTrue>
      </Operation>
    </caseTrue>
  </Operation>

  <Operation Class="XmlExtensions.FindMod">
    <mods>
      <li>vanillaexpanded.temperature</li>
    </mods>
    <packageId>True</packageId>
    <caseTrue>
      <Operation Class="XmlExtensions.OptionalPatch">
        <modId>Rhynia.Mod.Misc</modId>
        <key>VE_VTE_SelfFunctionController</key>
        <defaultValue>False</defaultValue>
        <caseTrue>
          <Operation Class="PatchOperationReplace">
            <xpath>/Defs/ThingDef[defName = "VTE_ACControlUnit"]/comps/li[@Class = "VanillaTemperatureExpanded.Comps.CompProperties_ResourceSingleton"]/consumptionPerTick</xpath>
            <value>
              <consumptionPerTick>-120</consumptionPerTick>
            </value>
          </Operation>
          <Operation Class="PatchOperationReplace">
            <xpath>/Defs/ThingDef[defName = "VTE_ACControlUnit"]/comps/li[@Class = "VanillaTemperatureExpanded.Comps.CompProperties_ResourceSingleton"]/idleConsumptionPerTick</xpath>
            <value>
              <idleConsumptionPerTick>-120</idleConsumptionPerTick>
            </value>
          </Operation>
        </caseTrue>
      </Operation>
    </caseTrue>
  </Operation>
</Patch>